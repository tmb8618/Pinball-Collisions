package gameCode
{
	/**
	 * VECT
	 * 
	 * cutsom made vector class
	 * Store the x and y components of the vetor
	 **/
	public class Vect
	{
		public var x:Number; // The x component of the vector
		public var y:Number; // the y component of the vector
		
		public function Vect(xIn:Number, yIn:Number)
		{
			this.x = xIn;
			this.y = yIn;
		}
	}
}